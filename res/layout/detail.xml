<?xml version="1.0" encoding="utf-8"?>
<TableLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:id="@+id/tableLayout1" android:layout_width="fill_parent" android:layout_height="fill_parent">
  <TableRow android:layout_width="fill_parent"> 
    <TextView android:text="First Name" android:id="@+id/textView1" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
    <EditText android:text="EditText" android:id="@+id/fname" android:layout_width="fill_parent" android:layout_height="wrap_content"></EditText>
  </TableRow>    
  <TableRow android:layout_width="fill_parent">   
    <TextView android:text="Last Name" android:id="@+id/textView1" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>    
    <EditText android:text="EditText" android:id="@+id/lname" android:layout_width="fill_parent" android:layout_height="wrap_content"></EditText>
</TableRow>
    <TableRow android:layout_width="fill_parent">
    <TextView android:text="address" android:id="@+id/textView1" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>    
    <EditText android:text="EditText" android:id="@+id/address" android:layout_width="fill_parent" android:layout_height="wrap_content"></EditText>
    </TableRow>
    <TableRow android:layout_width="fill_parent">
    <TextView android:text="phone" android:id="@+id/textView1" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
    <EditText android:text="EditText" android:id="@+id/phone" android:layout_width="fill_parent" android:layout_height="wrap_content"></EditText>
    </TableRow>
    <TableRow android:layout_width="fill_parent">
    <Button android:text="Submit" android:id="@+id/submit" android:layout_width="fill_parent" android:layout_height="wrap_content"></Button>
    </TableRow>
</TableLayout>
